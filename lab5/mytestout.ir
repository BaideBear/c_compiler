FUNCTION main :
READ v1
IF v1 <= #0 GOTO L2
LABEL L1 :
WRITE #1
GOTO L3
LABEL L2 :
IF v1 >= #0 GOTO L5
LABEL L4 :
WRITE #-1
GOTO L6
LABEL L5 :
WRITE #0
LABEL L6 :
LABEL L10000 :
GOTO L7
LABEL L7 :
IF #0 >= #5 GOTO L8
v5 := #0
LABEL L10001 :
GOTO L9
LABEL L9 :
IF v5 >= #3 GOTO L7
WRITE #0
WRITE v5
v7 := v5 + #1
v5 := v7
GOTO L9
LABEL L8 :
LABEL L3 :
RETURN #0

