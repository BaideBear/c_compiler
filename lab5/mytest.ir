FUNCTION main :
READ t0
v0 := t0
IF v0 > #0 GOTO L0
GOTO L1
LABEL L0 :
  WRITE #1
  GOTO L2
LABEL L1 :
  IF v0 < #0 GOTO L3
  GOTO L4
LABEL L3 :
  t1 := #0 - #1
  WRITE t1
  GOTO L5
LABEL L4 :
  WRITE #0
LABEL L5 :
  t2 := #0
  LABEL L6 :
    IF t2 >= #5 GOTO L8
    t3 := #0
    LABEL L7 :
      IF t3 >= #3 GOTO L6
      WRITE t2
      WRITE t3
      t3 := t3 + #1
      GOTO L7
    LABEL L8 :
LABEL L2 :
RETURN #0
